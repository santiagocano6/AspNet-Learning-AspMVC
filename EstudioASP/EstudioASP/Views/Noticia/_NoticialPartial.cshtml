@model IEnumerable<EstudioASP.Models.NoticiaModels>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TituloNoticia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DetalleNoticia)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TituloNoticia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DetalleNoticia)
        </td>
    </tr>
}

</table>
